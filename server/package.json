{
  "name": "server",
  "version": "1.0.0",
  "description": "Server for the rewards service",
  "keywords": [],
  "repository": {},
  "license": "ISC",
  "author": "",
  "main": "src/index.js",
  "scripts": {
    "dev": "nodemon src/index.js",
    "format": "npx prettier . --write",
    "lint": "npx eslint ./ --fix",
    "precommit-msg": "echo 'Server parsing...' && exit 0",
    "seed:database": "node src/dev-data/seeds/projectSeed.js",
    "sort": "npx sort-package-json",
    "start": "node src/index.js",
    "test": "env DEBUG=off NODE_ENV=test DATABASE_NAME=kstart_test mocha src/spec/**/*.js --exit",
    "test:coverage": "env DEBUG=off NODE_ENV=test DATABASE_NAME=kstart_test nyc --reporter=text mocha src/spec/**/*.js --exit",
    "test:coverageHtml": "env DEBUG=off NODE_ENV=test DATABASE_NAME=kstart_test nyc --reporter=html mocha src/spec/**/*.js --exit",
    "test:watch": "env DEBUG=off NODE_ENV=test DATABASE_NAME=kstart_test mocha src/spec/**/*.js --watch"
  },
  "pre-commit": [
    "precommit-msg",
    "sort",
    "lint",
    "format"
  ],
  "dependencies": {
    "aws-sdk": "^2.703.0",
    "axios": "^0.19.2",
    "commander": "^5.1.0",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.1.3",
    "faker": "^4.1.0",
    "helmet": "^3.23.1",
    "lodash": "^4.17.15",
    "mysql2": "^2.1.0",
    "random": "^2.2.0",
    "sequelize": "^5.21.13",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.10.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chalk": "^4.1.0",
    "chalk-pipe": "^4.0.0",
    "decache": "^4.6.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-chai-expect": "^2.1.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-mocha": "^7.0.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "inquirer": "^7.2.0",
    "mocha": "^8.0.1",
    "mocha-junit-reporter": "^2.0.0",
    "nodemon": "^2.0.4",
    "nyc": "^15.1.0",
    "prettier": "^2.0.5",
    "progress": "^2.0.3",
    "promise.bar": "^0.1.5",
    "proxyquire": "^2.1.3",
    "sequelize-test-helpers": "^1.2.2",
    "sinon": "^9.0.2",
    "sinon-chai": "^3.5.0",
    "supertest": "^4.0.2"
  },
  "engines": {
    "node": "12.16.3"
  }
}
